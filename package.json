{
  "name": "motif-space-subgraph",
  "version": "1.0.0",
  "scripts": {
    "create-graph": "graph create motifdev/motif-space-subgraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create motifdev/motif-space-subgraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen --output-dir types",
    "build": "graph build",
    "deploy": "graph deploy motifdev/motif-space-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy motifdev/motif-space-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@ethersproject/providers": "^5.0.16",
    "@fleekhq/fleek-storage-js": "^1.0.9",
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.0",
    "@types/jest": "^26.0.15",
    "@types/sjcl": "^1.0.29",
    "axios": "^0.21.0",
    "axios-retry": "^3.1.9",
    "dotenv": "^8.2.0",
    "ethers": "^5.0",
    "ganache-cli": "^6.12.2",
    "graphql-request": "^3.3.0",
    "mustache": "^4.1.0",
    "random-words": "^1.1.1",
    "sjcl": "^1.0.8",
    "system-commands": "^1.1.7",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@motifdev/listing": "1.0.7",
    "@motifdev/asset": "^1.0.7",
    "@motifdev/sdk": "^1.0.13"
  } 
}